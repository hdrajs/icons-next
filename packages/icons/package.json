{
  "name": "icons-next",
  "version": "1.0.3",
  "type": "module",
  "description": "icons library for react components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoanledev/icons-next.git"
  },
  "keywords": [
    "next-icons",
    "icons-next",
    "next",
    "nextjs",
    "icons park",
    "icons",
    "react icons",
    "icons for react components",
    "icons library",
    "react",
    "typescript"
  ],
  "author": "hoanledev",
  "license": "ISC",
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "typecheck": "tsc --noEmit",
    "build:icons": "tsx src/scripts/generate.ts",
    "format": "prettier --write \"src/**/*{.ts,.tsx}\"",
    "build": "pnpm build:icons && pnpm format",
    "pre:release": "tsc",
    "clean": "run-s clean:build clean:cache",
    "clean:build": "rm -rf dist",
    "clean:cache": "rm -rf node_modules .turbo .eslintcache stats.html",
    "release": "pnpm pre:release && pnpm publish"
  },
  "dist": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@repo/eslint-config": "workspace:*",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-virtual": "^3.0.1",
    "@rollup/pluginutils": "^5.0.4",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "prettier": "^3.3.3",
    "rollup": "^3.29.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.2",
    "@repo/typescript-config": "workspace:*",
    "xmldom": "^0.6.0",
    "tsx": "^4.16.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}